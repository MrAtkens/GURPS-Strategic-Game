{
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:33573"
      },
      "HttpsDefaultCert": {
        "Url": "https://*:44435"
      }
    }
  },
  "OpenIdConnect": {
    "name": "sso-flow",
    "authority": "https://id.epsd.test.kz/cas/oidc",
    "client_id": "gbimfront",
    "clientSecret": "megaSecretP@$$2020",
    "redirect_uri": "http://localhost:4200/authentication/login-callback",
    "post_logout_redirect_uri": "https://localhost:4200/authentication/logout-callback",
    "response_type": "id_token token",
    "scope": "openid profile email"
  },
  "services": {
    "profiles": {
      "host": "http://gbim-infrastructure-services.internal:6161"
    }
  },
  "accountApi": {
    "ApiEndpoint": "https://gbim-infrastructure-services.internal:44416/api/account",
    "TimeoutInSeconds": 40
  },
  "customSettings": {
    "EnableTracing": true
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq", "Serilog.Sinks.Graylog" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Warning",
          "outputTemplate": "[{Timestamp:HH:mm:ss.fff}] {Level:u3} {RequestId} {EventId} {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Graylog",
        "Args": {
          "hostnameOrAddress": "gbim-infrastructure-services.internal",
          "port": "12201",
          "transportType": "Udp"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
